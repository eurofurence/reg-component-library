{
  "name": "@eurofurence/reg-component-library",
  "version": "1.0.0",
  "description": "React UI components for the Eurofurence registration tools.",
  "author": "Eurofurence Registration Team",
  "license": "MIT",
  "repository": "eurofurence/reg-component-library",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --no-compress --format modern,cjs --jsxImportSource react",
    "start": "microbundle watch --no-compress --format modern,cjs --jsxImportSource react",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "tsc": "tsc",
    "microbundle": "microbundle"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@storybook/addon-actions": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.16",
    "@storybook/theming": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/ramda": "^0.27.36",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.2.2",
    "color-convert": "^2.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "gh-pages": "^3.1.0",
    "microbundle": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-scripts": "^4.0.2",
    "typescript": "^4.2.0-dev.20201119"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ramda": "^0.27.1"
  }
}
